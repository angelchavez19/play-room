generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  sub         String @id @unique
  name        String
  given_name  String
  family_name String
  picture     String
  email       String @unique
}
